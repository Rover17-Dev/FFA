{
  "name": "zsallazar/ffa",
  "description": "A simple ffa plugin for PocketMine-MP",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "phpstan/phpstan": "^1.10.26",
    "phpstan/phpstan-strict-rules": "^1.5.1",
    "phpstan/extension-installer": "^1.3.1",
    "pocketmine/pocketmine-mp": "^5.4.0"
  },
  "autoload": {
    "psr-4": {
      "": "/src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
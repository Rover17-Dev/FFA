{
  "name": "zsallazar/ffa",
  "description": "A simple ffa plugin for PocketMine-MP",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.3",
    "pocketmine/pocketmine-mp": "^5.17.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11.9",
    "phpstan/phpstan-strict-rules": "^1.6.0",
    "phpstan/extension-installer": "^1.4.1"
  },
  "autoload": {
    "psr-4": {
      "": "/src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
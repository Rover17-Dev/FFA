{
  "name": "zsallazar/ffa",
  "description": "A simple ffa plugin for PocketMine-MP",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.3",
    "pocketmine/pocketmine-mp": "^5.27.1",
    "dktapps/pmforms": "^2.3.0",
    "sof3/libasynql": "^4.2.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.12.24",
    "phpstan/phpstan-strict-rules": "^1.6.2",
    "phpstan/extension-installer": "^1.4.3"
  },
  "autoload": {
    "psr-4": {
      "": "/src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}